{
  "extends": [
    "airbnb-base",
    "plugin:jsx-a11y/recommended",
    "plugin:prettier/recommended",
    "plugin:unicorn/recommended"
  ],
  "env": {
    "browser": false
  },
  "overrides": [],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "unicorn/no-abusive-eslint-disable": 0,
    "unicorn/filename-case": [
      "error",
      {
        "cases": {
          "camelCase": true,
          "kebabCase": true
        }
      }
    ]
  }
}
